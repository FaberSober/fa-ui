{
  "name": "@fa/ui",
  "version": "0.0.1",
  "main": "./index.tsx",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css",
    "./index.css": "./dist/index.css"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "rimraf /s/d dist && rimraf /s/d .turbo",
    "ncu": "ncu -u --timeout 120000",
    "clean": "rimraf /s/d node_modules dist .turbo",
    "dev": "concurrently \"tsup --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
    "build": "concurrently \"tsup\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css\"",
    "dev:tsup": "tsup --watch",
    "build:tsup": "tsup",
    "lint": "eslint \"**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@dnd-kit/core": "6.0.8",
    "@dnd-kit/sortable": "7.0.2",
    "@dnd-kit/utilities": "3.2.1",
    "qs": "6.11.2",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "5.1.4",
    "@fa/eslint-config-custom": "workspace:*",
    "@fa/tailwind-config": "workspace:*",
    "@fa/tsconfig": "workspace:*",
    "@tinymce/tinymce-react": "4.3.0",
    "@types/lodash": "4.14.195",
    "@types/qs": "6.9.7",
    "@types/react": "18.2.14",
    "@types/uuid": "9.0.2",
    "ahooks": "3.7.8",
    "antd": "5.6.4",
    "autoprefixer": "10.4.14",
    "axios": "1.4.0",
    "concurrently": "8.2.0",
    "dayjs": "1.11.9",
    "eslint": "8.44.0",
    "fa-cron-react-editor": "0.0.13",
    "filesize": "10.0.7",
    "lodash": "4.17.21",
    "postcss": "8.4.24",
    "postcss-scss": "4.0.6",
    "react": "18.2.0",
    "react-contexify": "6.0.0",
    "react-device-detect": "2.2.3",
    "react-use": "17.4.0",
    "tailwindcss": "3.3.2",
    "tsup": "7.1.0",
    "typescript": "5.1.6",
    "use-bus": "2.5.2"
  }
