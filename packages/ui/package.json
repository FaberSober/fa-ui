{
  "name": "@fa/ui",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css",
    "./index.css": "./dist/index.css"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "rimraf /s/d dist && rimraf /s/d .turbo",
    "ncu": "ncu -u --timeout 120000",
    "clean": "rimraf /s/d node_modules dist .turbo",
    "dev": "concurrently \"tsup --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
    "build": "concurrently \"tsup\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css\"",
    "dev:tsup": "tsup --watch",
    "build:tsup": "tsup",
    "lint": "eslint \"**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/sortable": "8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "chroma-js": "3.1.2",
    "crypto-js": "4.2.0",
    "qs": "6.13.0",
    "uuid": "11.0.3"
  },
  "devDependencies": {
    "@ant-design/icons": "5.5.1",
    "@fa/eslint-config-custom": "workspace:*",
    "@fa/tailwind-config": "workspace:*",
    "@fa/tsconfig": "workspace:*",
    "@tinymce/tinymce-react": "5.1.1",
    "@types/chroma-js": "2.4.4",
    "@types/crypto-js": "4.2.2",
    "@types/lodash": "4.17.13",
    "@types/qs": "6.9.17",
    "@types/react": "18.3.12",
    "@types/uuid": "10.0.0",
    "ahooks": "3.8.1",
    "antd": "5.22.1",
    "autoprefixer": "10.4.20",
    "axios": "1.7.7",
    "concurrently": "9.1.0",
    "dayjs": "1.11.13",
    "eslint": "9.14.0",
    "fa-cron-react-editor": "0.0.13",
    "filesize": "10.1.6",
    "lodash": "4.17.21",
    "postcss": "8.4.49",
    "postcss-scss": "4.0.9",
    "react": "18.3.1",
    "react-contexify": "6.0.0",
    "react-use": "17.5.1",
    "tailwindcss": "3.4.14",
    "tsup": "8.3.5",
    "typescript": "5.6.3",
    "use-bus": "2.5.2"
  }
