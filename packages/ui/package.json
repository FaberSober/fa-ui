{
  "name": "@fa/ui",
  "version": "0.0.1",
  "main": "./index.tsx",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css",
    "./index.css": "./dist/index.css"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "rimraf /s/d dist && rimraf /s/d .turbo",
    "ncu": "ncu -u --timeout 120000",
    "clean": "rimraf /s/d node_modules dist .turbo",
    "dev": "concurrently \"tsup --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
    "build": "concurrently \"tsup\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css\"",
    "dev:tsup": "tsup --watch",
    "build:tsup": "tsup",
    "lint": "eslint \"**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/sortable": "8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "qs": "6.11.2",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@ant-design/icons": "5.2.6",
    "@fa/eslint-config-custom": "workspace:*",
    "@fa/tailwind-config": "workspace:*",
    "@fa/tsconfig": "workspace:*",
    "@tinymce/tinymce-react": "4.3.2",
    "@types/lodash": "4.14.201",
    "@types/qs": "6.9.10",
    "@types/react": "18.2.37",
    "@types/uuid": "9.0.7",
    "ahooks": "3.7.8",
    "antd": "5.11.2",
    "autoprefixer": "10.4.16",
    "axios": "1.6.2",
    "concurrently": "8.2.2",
    "dayjs": "1.11.10",
    "eslint": "8.54.0",
    "fa-cron-react-editor": "0.0.13",
    "filesize": "10.1.0",
    "lodash": "4.17.21",
    "postcss": "8.4.31",
    "postcss-scss": "4.0.9",
    "react": "18.2.0",
    "react-contexify": "6.0.0",
    "react-use": "17.4.0",
    "tailwindcss": "3.3.5",
    "tsup": "8.0.0",
    "typescript": "5.3.2",
    "use-bus": "2.5.2"
  }
}
