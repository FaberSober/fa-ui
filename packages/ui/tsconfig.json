{
  "extends": "@fa/tsconfig/react-library.json",
  "include": ["src", "."],
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@ui/*": ["./src/*"]
    },
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "composite": true,
    "tsBuildInfoFile": "./dist/.tsbuildinfo"
  }
}
